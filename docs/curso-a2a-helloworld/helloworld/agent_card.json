{
  "name": "AgenteSaudacao",
  "version": "1.0.0",
  "description": "Agente simples que cumprimenta pessoas",
  "skills": [
    "greet",
    "welcome"
  ],
  "endpoints": {
    "agent_card": "/.well-known/agent-card",
    "tasks": "/api/v1/tasks",
    "messages": "/api/v1/tasks/{task_id}/messages"
